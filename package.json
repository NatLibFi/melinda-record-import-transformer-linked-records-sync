{
	"name": "@natlibfi/melinda-record-import-transformer-record-link-migration",
	"description": "Record link migration transformer for the Melinda record batch import system",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/NatLibFi/melinda-record-import-transformer-record-link-migration",
	"bugs": {
		"url": "https://github.com/NatLibFi/melinda-record-import-transformer-record-link-migration/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:NatLibFi/melinda-record-import-transformer-record-link-migration.git"
	},
	"license": "AGPL-3.0+",
	"version": "0.0.1",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "node dist/ndex.js",
		"cli": "node dist/cli.js",
		"check-deps": "npm-check;exit 0",
		"lint": "eslint src",
		"lint:dev": "eslint --fix src",
		"test": "npm run lint",
		"build": "babel src --source-maps --delete-dir-on-start --out-dir=dist --minified",
		"build:dev": "npm run build:transpile && npm run check-deps",
		"watch": "cross-env DEBUG=1 NODE_ENV=debug nodemon --watch src --exec 'npm test'"
	},
	"dependencies": {
		"@natlibfi/marc-record": "^4.0.0",
		"@natlibfi/melinda-commons": "7.1.0",
		"@natlibfi/melinda-record-import-commons": "^7.0.1",
		"moment": "^2.22.2",
		"stream-chain": "^2.2.1",
		"stream-json": "^1.3.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/node": "^7.7.4",
		"@babel/preset-env": "^7.7.6",
		"@babel/register": "^7.7.4",
		"babel-eslint": "^10.0.3",
		"cross-env": "^6.0.3",
		"eslint": "^6.7.2",
		"eslint-config-xo": "^0.27.2",
		"eslint-plugin-import": "^2.19.1",
		"nodemon": "^2.0.1",
		"npm-check": "^5.9.0"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"plugins": [
			"import"
		],
		"extends": [
			"xo",
			"plugin:import/errors"
		],
		"rules": {
			"prefer-arrow-callback": [
				"error"
			]
		}
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "10"
					}
				}
			]
		]
	}
}
